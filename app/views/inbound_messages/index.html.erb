<h1>Listing inbound_messages</h1>

<table>
  <tr>
    <th>Range</th>
    <th>Message</th>
    <th>Sender</th>
    <th>Recipient</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @inbound_messages.each do |inbound_message| %>
  <tr>
    <td><%= inbound_message.range_id %></td>
    <td><%= inbound_message.message %></td>
    <td><%= inbound_message.sender %></td>
    <td><%= inbound_message.recipient %></td>
    <td><%= link_to 'Show', inbound_message %></td>
    <td><%= link_to 'Edit', edit_inbound_message_path(inbound_message) %></td>
    <td><%= link_to 'Destroy', inbound_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Inbound message', new_inbound_message_path %>
